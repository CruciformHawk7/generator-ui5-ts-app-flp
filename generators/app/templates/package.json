{
	"name": "<%= namespace %>",
	"version": "1.0.0",
	"description": "UI5 Application: <%= namespace %>",
	"author": "<%= author %>",
	"license": "Apache-2.0",
	"scripts": {
		"clean": "rimraf dist coverage",
		"build": "ui5 build --clean-dest",
		"build:opt": "ui5 build self-contained --clean-dest --all",
		"start": "ui5 serve --port 8080 -o flpSandbox/flpSandbox.html",
		"start-cdn": "ui5 serve --port 8080 -o index-cdn.html",
		"start:dist": "npm start -- --config ui5-dist.yaml",
		"start:dist-cdn": "npm run start-cdn -- --config ui5-dist.yaml",
		"gen-models": "odata2ts",
		"ts-typecheck": "tsc --noEmit",
		"lint": "eslint webapp",
		"karma": "karma start",
		"karma-ci": "karma start karma-ci.conf.js",
		"karma-ci-cov": "karma start karma-ci-cov.conf.js",
		"test": "npm run lint && npm run karma-ci-cov"
	},
	"devDependencies": {
		"<%= tstypes %>": "<%= tstypesVersion %>",
		"@odata2ts/odata2ts": "^0.40.1",
		"@typescript-eslint/eslint-plugin": "^8.50.0",
		"@typescript-eslint/parser": "^8.50.0",
		"@sap/ux-ui5-tooling": "^1.20.0",
		"@ui5/cli": "^4.0.38",
		"eslint": "^9.39.2",
		"karma": "^6.4.4",
		"karma-chrome-launcher": "^3.2.0",
		"karma-coverage": "^2.2.1",
		"karma-ui5": "^4.1.0",
		"karma-ui5-transpile": "^3.6.0",
		"rimraf": "^6.1.2",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.51.0",
		"ui5-middleware-livereload": "^3.2.0",
		"ui5-tooling-transpile": "^3.10.0"
	}
}
